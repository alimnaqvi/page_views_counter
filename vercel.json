{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "python3 init_db.py",
  "devCommand": "uvicorn main:app --reload",
  "framework": null,
  "outputDirectory": ".",
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ]
}
